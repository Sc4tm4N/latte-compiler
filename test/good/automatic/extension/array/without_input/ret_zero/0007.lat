int main() {
  boolean[] a = new boolean[3];

  for(boolean b : a) {
    printBoolean(b);
    b = true;
    printBoolean(b);
  }

  for(boolean b : a) {
    printBoolean(b);
  }

  return 0;
}

void printBoolean(boolean b) {
  if(b) {
    printString("true");
  } else {
    printString("false");
  }
}